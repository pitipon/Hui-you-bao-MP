<!--pages/group/group.wxml-->

<!-- <view class="title">Recent | Popular | Category</view> -->

<!--CARDDDDDDD SECTION  -->
<view class="card">
  <view class="flex-wrp">

    <!-- HEAD PART  -->
    <view class="flex-item-head ">
      <view class="card-head">
          <!-- HEAD LEFT (avatar) -->
          <view class="card-head-left">
              <image class="avatar" src='../../dist/images/mo.jpg'></image>
          </view>
          <!-- HEAD RIGHT (name + discount)  -->
          <view class="card-head-right">
            <view class="card-head-between-name-discount">
              <!-- FOR NAME AND LOCATION  -->
              <view class='flex-item-name'> 
                <view class='flex-for-name-time'>
                  <view class='name-font-head'>Mo-Pitipon</view>
                  <view class='detail-font-head'>
                  5 mins ago at BKK salmon, Thailand</view>
                </view>  
              </view>
              <!-- FOR NAME AND LOCATION  -->

              <!-- FOR DISCOUNT  -->
              <view >
                <view class="discount-avatar " style="background-image: linear-gradient(rgba(0, 0, 0, 0.2), rgba(0, 0, 0, 0.1)), url('../../dist/images/salmon1.jpg')">
                    <view class='discount-text'>
                      7折
                    </view>  
                </view>
              </view>
              <!-- FOR DISCOUNT  -->
            </view>
         </view>
         <!-- HEAD RIGHT (name + discount)  -->
      </view>
    </view>
    <view class="flex-item-msg">Sooo dam good , $1 </view>
    <!-- HEAD PART  -->

    <!-- BODY PART  -->
    <view class="flex-item-pic " style="background-image: linear-gradient(rgba(0, 0, 0, 0.1), rgba(0, 0, 0, 0.2)), url('../../dist/images/salmon1.jpg')"></view>
    <!-- BODY PART  -->

    <!-- BOTTOM PART  -->
    <view class="flex-item-bottom">
       
        <view class='flex-bottom-item'>❤ 10万</view>
        <view class='flex-bottom-item'>Comment</view>
        <view class='flex-bottom-item'>Share</view>
    </view>
    <!-- BOTTOM PART  -->
  </view>
</view>
